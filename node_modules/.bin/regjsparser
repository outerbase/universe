#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brayden/Documents/Outerbase/universe/node_modules/regjsparser/bin/node_modules:/Users/brayden/Documents/Outerbase/universe/node_modules/regjsparser/node_modules:/Users/brayden/Documents/Outerbase/universe/node_modules:/Users/brayden/Documents/Outerbase/node_modules:/Users/brayden/Documents/node_modules:/Users/brayden/node_modules:/Users/node_modules:/node_modules:/Users/brayden/Documents/Outerbase/universe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brayden/Documents/Outerbase/universe/node_modules/regjsparser/bin/node_modules:/Users/brayden/Documents/Outerbase/universe/node_modules/regjsparser/node_modules:/Users/brayden/Documents/Outerbase/universe/node_modules:/Users/brayden/Documents/Outerbase/node_modules:/Users/brayden/Documents/node_modules:/Users/brayden/node_modules:/Users/node_modules:/node_modules:/Users/brayden/Documents/Outerbase/universe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
